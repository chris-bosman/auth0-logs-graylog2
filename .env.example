#AUTH0 LOGGER ACCOUNT SETTINGS
AUTH0_CLIENT_ID=
AUTH0_CLIENT_SECRET=
AUTH0_DOMAIN=

# NUMBER OF LOGS PER REQUEST
BATCH_SIZE=100

# START FROM A SPECIFIC LOG ID
START_FROM_ID=null

# FREQUENCY OF LOG POLLING IN SECONDS
POLLING_INTERVAL_IN_SEC=1

# FOLLOW THE RECENT LOG QUEUE WITH SOME DELAY FOR LOGS TO BE INDEXED CORRECTLY IN AUTH0
TRACK_THE_LATEST_IN_SEC=600

# Leave it blank if you want all clients in the logs. This is a sample with some dummy client IDs. Add yours!
FILTER_CLIENTS_WITH_ID=7Z6WTmeFYmLK0Hr7GwIXDyOKXRFFmUV1, yB2FbvhXyR4OWiSAAOk8lmRGfrsLNJm2, m45FbvhXyR4OWiSAAOk533RGfrsLNJm2

# OPTIONS FOR GREYLOG2
GRAYLOG2_NAME=Graylog
GRAYLOG2_HOST=
GRAYLOG2_PORT=
GRAYLOG2_BUFFERSIZE=1400
GRAYLOG2_ENABLE=true
GRAYLOG2_STATIC_META=Some Meta Data About This Log

# OPTIONS FOR FILE LOGGER
FILELOG_ENABLE=false